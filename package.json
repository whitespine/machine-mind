    "pretty-quick": "^1.11.1",
    "raw-loader": "^4.0.0",
    "source-map-support": "^0.5.10",
    "standard-changelog": "^2.0.21",
    "ts-jest": "^26.4.1",
    "ts-loader": "^6.2.1",
    "ts-transformer-keys": "^0.4.2",
    "tscpaths": "0.0.9",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4"
