  "license": "GPL-3.0-or-later",
  "author": "whitespine",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "unit-test": "jest unit",
    "commit": "git-cz",
    "format": "prettier --write --config .prettierrc.json 'src/**/*.ts'",
    "build": "ttsc && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "build:watch": "ttsc -w --noEmit",
