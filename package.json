    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@types/jest": "^26.0.15",
    "@types/jszip": "^3.1.6",
    "@types/nanoid": "^2.1.0",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "core-js": "3",
