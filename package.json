{
  "name": "machine-mind",
  "version": "0.1.3",
  "description": "Backing data processing types for compcon - modification of existing code from the official project",
  "license": "GPL-3.0-or-later",
  "author": "whitespine",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "unit-test": "jest unit",
    "commit": "git-cz",
    "format": "prettier --write --config .prettierrc.json 'src/**/*.ts'",
    "build": "tsc && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "build:watch": "tsc -w --noEmit",
    "link": "npm run build && npm link",
    "test": "echo No tests",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run format"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jszip": "^3.5.0",
    "lodash": "^4.17.11",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@types/jest": "^24.0.13",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.123",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "core-js": "3",
    "cz-customizable": "^6.2.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-loader": "^3.0.3",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "jest": "^25.1.0",
    "jest-mock-random": "^1.0.2",
    "lancer-data": "^2.0.33",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "raw-loader": "^4.0.0",
    "source-map-support": "^0.5.10",
    "standard-changelog": "^2.0.21",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.1",
    "tscpaths": "0.0.9",
    "typescript": "^3.7.5",
    "typescript-json-schema": "^0.42.0"
  },
  "resolutions": {
    "prosemirror-model": "1.8.2"
